{
    "experiment_name": "refactoring",

    "model": "fasterrcnn",
    "initialization": "backbone",
    "reuse_classifier": false,

    "learning_rate": 0.02,
    "num_warmup_steps": 250,
    "initial_total_steps": 500,
    "total_steps": 500,

    "momentum": 0.9,
    "gradient_clip_threshold": 0.1,
    "seed": 47,
    "weight_decay": 0.0001,
    "batch_size": 1,
    "confidence_threshold": 0.9,
    "validation_part": 0,
    "num_workers": 0,

    "consistency_lambda": 2,
    "consistency_criterion": "KL"

}
