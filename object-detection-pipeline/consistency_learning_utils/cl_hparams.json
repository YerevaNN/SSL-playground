{
    "experiment_name": "doesnotmatter",

    "model": "fasterrcnn",
    "initialization": "backbone",
    "reuse_classifier": "add",

    "total_steps_teacher_initial": 40000,
    "total_steps_student_initial": 40000,
    "total_steps_teacher_stage_inc": 0,
    "total_steps_student_stage_inc": 0,
    "check_val_steps": 1000,

    "learning_rate": 0.01,
    "lr_drop_rate": 10,
    "warmup_steps": 1000,
    "lr_drop_steps": 30000,
    "lr_schedule": "warmupWithDrop",
    "momentum": 0.9,
    "weight_decay": 0,

    "student_learning_rate": 0.01,
    "student_lr_drop_rate": 10,
    "student_warmup_steps": 1000,
    "student_lr_drop_steps": 40000,
    "student_lr_schedule": "warmupWithDrop",

    "augmentation": 1,
    "confidence_threshold": 0.9,
    "box_score_thresh": 0.05,
    "EMA_keep_rate": 0.996,
    "batch_size": 5,
    "gradient_clip_threshold": 0.5,

    "seed": 47,
    "validation_part": 0,
    "num_workers": 8,

    "consistency_lambda": 2,
    "consistency_criterion": "KL"
}
