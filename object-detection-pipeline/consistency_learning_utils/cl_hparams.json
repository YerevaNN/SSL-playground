{
    "experiment_name": "refactoring",

    "model": "fasterrcnn",
    "initialization": "backbone",
    "reuse_classifier": false,

    "learning_rate": 0.02,
    "total_steps_teacher": 800,
    "total_steps_student": 800,
    "check_val_steps": 100,
    "warmup_steps": 200,
    "lr_schedule": "constant",

    "momentum": 0.9,
    "gradient_clip_threshold": 0.1,
    "seed": 47,
    "weight_decay": 0.0001,
    "batch_size": 1,
    "confidence_threshold": 0.9,
    "validation_part": 0,
    "num_workers": 0,

    "consistency_lambda": 2,
    "consistency_criterion": "KL"

}
