{
    "experiment_name": "debug-teacher",

    "model": "fasterrcnn",
    "initialization": "full",
    "reuse_classifier": "add",

    "learning_rate": 0.01,
    "total_steps_teacher": 20,
    "total_steps_student": 8000,
    "check_val_steps": 80,
    "warmup_steps": 200,
    "lr_schedule": "constant",

    "momentum": 0.9,
    "gradient_clip_threshold": 0.1,
    "seed": 47,
    "weight_decay": 0,
    "batch_size": 6,
    "confidence_threshold": 0.9,
    "validation_part": 0,
    "num_workers": 8,

    "consistency_lambda": 2,
    "consistency_criterion": "KL"

}
